#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/anzhiyu/Project/2025/anheyu-work/anheyu-newtab/node_modules/.pnpm/fx-runner@1.4.0/node_modules/fx-runner/bin/node_modules:/Users/anzhiyu/Project/2025/anheyu-work/anheyu-newtab/node_modules/.pnpm/fx-runner@1.4.0/node_modules/fx-runner/node_modules:/Users/anzhiyu/Project/2025/anheyu-work/anheyu-newtab/node_modules/.pnpm/fx-runner@1.4.0/node_modules:/Users/anzhiyu/Project/2025/anheyu-work/anheyu-newtab/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/anzhiyu/Project/2025/anheyu-work/anheyu-newtab/node_modules/.pnpm/fx-runner@1.4.0/node_modules/fx-runner/bin/node_modules:/Users/anzhiyu/Project/2025/anheyu-work/anheyu-newtab/node_modules/.pnpm/fx-runner@1.4.0/node_modules/fx-runner/node_modules:/Users/anzhiyu/Project/2025/anheyu-work/anheyu-newtab/node_modules/.pnpm/fx-runner@1.4.0/node_modules:/Users/anzhiyu/Project/2025/anheyu-work/anheyu-newtab/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fx-runner/bin/fx-runner" "$@"
else
  exec node  "$basedir/../fx-runner/bin/fx-runner" "$@"
fi
